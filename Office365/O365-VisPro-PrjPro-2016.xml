<Configuration>
    <Info Description="Office 365 with Visio Pro and Project Pro 2016" />
    <Add OfficeClientEdition="32" Channel="Broad" OfficeMgmtCOM="TRUE" Version="16.0.10730.20334" ForceUpgrade="TRUE" AllowCdnFallback="True">
        <Product ID="O365ProPlusRetail">
            <Language ID="MatchOS" Fallback="en-us" />
            <Language ID="es-es" />
            <Language ID="pt-br" />
            <Language ID="MatchPreviousMSI" />
            <ExcludeApp ID="Groove" />
        </Product>
        <Product ID="VisioProXVolume">
            <Language ID="en-us" />
            <Language ID="es-es" />
            <Language ID="pt-br" />
            <Language ID="MatchPreviousMSI" />
        </Product>
        <Product ID="ProjectProXVolume">
            <Language ID="en-us" />
            <Language ID="es-es" />
            <Language ID="pt-br" />
            <Language ID="MatchPreviousMSI" />
        </Product>
    </Add>
    <RemoveMSI All="TRUE" />
    <Remove All="FALSE">
        <Product ID="VisioStdXVolume" />
        <Product ID="ProjectStdXVolume" />
    </Remove>
    <AppSettings>
        <Setup Name="Company" Value="Contoso" />
    </AppSettings>
    <Property Name="FORCEAPPSHUTDOWN" Value="TRUE" />
    <Display Level="Full" AcceptEULA="TRUE" />
    <Logging Level="Standard" Path="%TEMP%\Office365" />
</Configuration>
